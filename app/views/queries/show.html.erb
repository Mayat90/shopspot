<%= render 'shared/navbar' %>
<div id="showcontent">
  <div id="showleft">
    <div id=showsearch>
      <%= render 'shared/show_search' %>
    </div>

    <div id="showmapframe">
      <div id="showmapcontent">
        <%= static_map_for(@query) %>
      </div>
    </div>
  </div>
  <div id="showcard">
    <%= render 'shared/show_card' %>
  </div>
  <div id="showright">
    <div id="linksframe">
      <div id="linkscontent">
        <div id="linkstitle">
          <h3>Want more ?</h3>
        </div>
        <%= link_to 'Cities analytics', query_cities_path(@query), class: 'btn btn-primary', id:'btn-show' %>
        <%= link_to 'View competition', query_competitors_path(@query), class: 'btn btn-primary', id:'btn-show' %>
      </div> 
    </div>
    <div id="showstreetframe">
      <div id="showstreetcontent">
        <%= street_view(@query) %>
      </div>
    </div>
  </div>
</div>



